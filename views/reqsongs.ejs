<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <title>Song Requests</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
 </head>

  <body>
  <h1>Song Requests</h1>

 
  
  <table cellspacing="1" cellpadding="5" border="1">
    <tr>
    <th>Request Song</th>
    <th>Action</th>
  </tr>


  <% data.forEach(function(reqsongs){ %>
     <tr>
       <td><%= reqsongs.reqsong %></td>
       <td><a href="editreqsong/<%= reqsongs.doc_id %>">Edit</a> | Delete</td>
     </tr>
  <% }); %>

	</table>

 
 

  </body>

</html>